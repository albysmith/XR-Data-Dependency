        <library name="Create">
            <actions>
                <do_all exact="$Macros.count" counter="$i">
                    <create_ship groupname="md.$ThisBlows" name="$Ship" zone="player.zone" macro="$Macros.{$i}">
                        <select faction="faction.argon" />
                        <pilot macro="character_boron_generic_01_macro">
                            <skills>
                                <skill type="piloting" exact="15" />
                                <skill type="boarding" exact="15" />
                                <skill type="engineering" exact="15" />
                                <skill type="management" exact="15" />
                                <skill type="morale" exact="15" />
                            </skills>
                        </pilot>
                        <loadout>
                            <level exact="1.0" />
                            <variation exact="0.0" />
                        </loadout>
                        <owner exact="faction.argon"></owner>
                        <!-- <position x="$Macros.{$i}.width + ($i * 2km) + 10km" /> -->
                        <!-- <position x="($i * $X_Off) + 10km" y="$Y_Off" /> -->
                        <safepos object="player.ship" z="10000m" />
                    </create_ship>
                    <create_order object="$Ship" id="'Wait'" default="true" immediate="true" />
                    <set_owner object="$Ship" faction="faction.player" overridenpc="true" />
                    <signal_cue_instantly cue="YouMustWait" param="$Ship" />
                </do_all>
            </actions>
        </library>
